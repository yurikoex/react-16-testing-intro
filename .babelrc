{
        "plugins": [
                "transform-react-jsx",
                "transform-object-rest-spread",
                [
                        "transform-regenerator",
                        {
                                "asyncGenerators": true,
                                "generators": true,
                                "async": true
                        }
                ]
        ],
        "presets": [
                [
                        "env",
                        {
                                "targets": {
                                        "node": "current"
                                }
                        }
                ]
        ],
        "ignore": ["node_modules"]
}
